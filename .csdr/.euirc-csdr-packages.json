{
    "devops-metadata": {
        "name": "devops-metadata",
        "repository": "github.com/ec-europa-eui/csdr-devops-metadata.git",
        "build": {
            "exec": false
        }
    },

    "eui-tools": {
        "name": "eui-tools",
        "npmPkg": "@eui/tools",
        "build": {
            "exec": false,
            "generateDoc": false,
            "sonarScanner": false,
            "masterBranchOnly": true,
            "ownDepsOnly": true,
            "prebuildScript": true,
            "postbuildScript": true,
            "publicRegistry": "https://registry.npmjs.org"
